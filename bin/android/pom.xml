<project 	xmlns="http://maven.apache.org/POM/4.0.0" 	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
		<modelVersion>4.0.0</modelVersion>

	<groupId>com.adobe.mobile</groupId>
	<artifactId>ADEPMobileAndroidNE</artifactId>
	<version>1.0-SNAPSHOT</version>
	<packaging>jar</packaging>

	<name>Adobe Digital Enterprise Platform - Mobile</name>
	<url>http://maven.apache.org</url>

	<properties>
		<project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
	</properties>	<build>		<plugins>			<plugin>				<groupId>org.apache.maven.plugins</groupId>				<artifactId>maven-install-plugin</artifactId>				<version>2.3.1</version>				<executions>					<execution>						<id>install FlashRuntimeExtensions.jar</id>						<goals>							<goal>install-file</goal>						</goals>						<phase>validate</phase>						<configuration>							<file>${env.FLEX_HOME}/lib/android/FlashRuntimeExtensions.jar</file>							<groupId>com.adobe.platform.flash</groupId>							<artifactId>FlashRuntimeExtensions</artifactId>							<version>3.0.0</version>							<packaging>jar</packaging>						</configuration>					</execution>				</executions>			</plugin>		</plugins>	</build>	<dependencies>
		<dependency>
			<groupId>junit</groupId>
			<artifactId>junit</artifactId>
			<version>3.8.1</version>
			<scope>test</scope>
		</dependency>
		<dependency>
			<groupId>com.google.android</groupId>
			<artifactId>android</artifactId>
			<version>2.2.1</version>
			<scope>provided</scope>
		</dependency>
		<dependency>
			<groupId>com.google.android</groupId>
			<artifactId>android-test</artifactId>
			<version>2.2.1</version>
			<scope>provided</scope>
		</dependency>
		<dependency>
			<groupId>com.adobe.platform.flash</groupId>
			<artifactId>FlashRuntimeExtensions</artifactId>
			<version>3.0.0</version>
		</dependency>
	</dependencies>

</project>
